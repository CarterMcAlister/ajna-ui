{
  "name": "@ajna-ui/core",
  "version": "1.0.0",
  "homepage": "https://github.com/CarterMcAlister/ajna-ui#readme",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup ./src/index.ts --format esm,cjs --dts --external react",
    "dev": "tsup ./src/index.ts --format esm,cjs --watch --dts --external react",
    "lint": "TIMING=1 eslint src/**/*.ts* --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CarterMcAlister/ajna-ui.git",
    "directory": "packages/form"
  },
  "license": "MIT",
  "author": "Carter McAlister",
  "keywords": [],
  "dependencies": {
    "@ajna-ui/form": "*",
    "@ajna-ui/form-checkbox": "*",
    "@ajna-ui/form-error-message": "*",
    "@ajna-ui/form-input": "*",
    "@ajna-ui/form-item": "*",
    "@ajna-ui/form-markdown-editor": "*",
    "@ajna-ui/form-number-field": "*",
    "@ajna-ui/form-select": "*",
    "@ajna-ui/form-submit-button": "*",
    "@ajna-ui/form-switch": "*",
    "@ajna-ui/form-textarea": "*",
    "@ajna-ui/lottie": "*",
    "@ajna-ui/motion-box": "*",
    "@ajna-ui/table": "*"
  },
  "peerDependencies": {
    "@chakra-ui/react": "^2.0.0",
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@chakra-ui/react": "^2.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
